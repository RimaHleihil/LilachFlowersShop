<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER" prefHeight="1746.0" prefWidth="860.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.Catalogcontrol">
    <children>
        <AnchorPane prefHeight="1746.0" prefWidth="860.0" style="-fx-background-color: DarkSeaGreen;">
            <children>
                <Button fx:id="homebutton" layoutX="74.0" layoutY="57.0" mnemonicParsing="false" onAction="#goHome" prefHeight="31.0" prefWidth="66.0" style="-fx-background-color: white;" text="Home">
                    <font>
                        <Font name="Century Gothic Bold" size="12.0" />
                    </font></Button>
                <TableView fx:id="ProductTable" layoutX="45.0" layoutY="271.0" prefHeight="434.0" prefWidth="770.0" style="-fx-background-color: white;">
                    <columns>
                        <TableColumn fx:id="imageCol" prefWidth="281.60001826286316" resizable="false" text="Image" />
                        <TableColumn fx:id="productName" prefWidth="280.0000305175781" text="Name" />
                        <TableColumn fx:id="productKind" prefWidth="103.2000732421875" text="Kind" />
                        <TableColumn fx:id="productPrice" prefWidth="179.199951171875" text="Price" />
                    </columns>
                </TableView>
                <Button fx:id="showProductDetails" layoutX="21.0" layoutY="137.0" mnemonicParsing="false" onAction="#showDetails" prefHeight="31.0" prefWidth="106.0" style="-fx-background-color: white;" text="Show Details">
                    <font>
                        <Font name="Century Gothic Bold" size="12.0" />
                    </font></Button>
                <Button fx:id="editButton" layoutX="267.0" layoutY="137.0" mnemonicParsing="false" onAction="#goEdit" prefHeight="31.0" prefWidth="105.0" style="-fx-background-color: white;" text="Edit">
                    <font>
                        <Font name="Century Gothic Bold" size="12.0" />
                    </font></Button>
