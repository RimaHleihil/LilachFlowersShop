                    <font>
                        <Font name="Century Gothic Bold" size="12.0" />
                    </font></Button>
                <Button fx:id="deleteButton" layoutX="538.0" layoutY="137.0" mnemonicParsing="false" onAction="#deleteProudct" prefHeight="31.0" prefWidth="119.0" style="-fx-background-color: white;" text="Delete Product">
                    <font>
                        <Font name="Century Gothic Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="discountButton" layoutX="693.0" layoutY="137.0" mnemonicParsing="false" onAction="#addDiscount" prefHeight="31.0" prefWidth="158.0" style="-fx-background-color: white;" text="Add Discount">
                    <font>
                        <Font name="Century Gothic Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="Add2Pascket" layoutX="141.0" layoutY="137.0" mnemonicParsing="false" onAction="#AddToPascket" prefHeight="31.0" prefWidth="106.0" style="-fx-background-color: white;" text="Add To Pascket">
                    <font>
                        <Font name="Century Gothic Bold" size="12.0" />
                    </font>
                </Button>
            </children>
        </AnchorPane>
    </children>
</HBox>
