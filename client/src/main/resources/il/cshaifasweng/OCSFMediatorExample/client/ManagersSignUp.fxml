                </Label>
                <Label layoutX="129.0" layoutY="149.0" text="Name">
                    <font>
                        <Font name="Century Gothic" size="18.0" />
                    </font>
                </Label>
                <Label layoutX="129.0" layoutY="189.0" text="ID">
                    <font>
                        <Font name="Century Gothic" size="18.0" />
                    </font>
                </Label>
                <Label fx:id="password" layoutX="129.0" layoutY="229.0" text="Password">
                    <font>
                        <Font name="Century Gothic" size="18.0" />
                    </font>
                </Label>
                <TextField fx:id="nameTF" layoutX="294.0" layoutY="148.0" />
