            <Font name="Arial" size="14.0" />
         </font></Label>
      <Label fx:id="welcomeText1" layoutX="211.0" layoutY="269.0" prefHeight="17.0" prefWidth="199.0" style="-fx-alignment: yellow;" text="already a client? Sign in!" textAlignment="CENTER" textFill="#9a226a">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
      </Label>
      <ImageView fitHeight="77.0" fitWidth="377.0" layoutX="122.0" layoutY="86.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@lil.jpg" />
         </image>
      </ImageView>
