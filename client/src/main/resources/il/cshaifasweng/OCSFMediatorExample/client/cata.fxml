<HBox alignment="CENTER" prefHeight="604.0" prefWidth="615.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.catacontrol">
   <AnchorPane prefHeight="604.0" prefWidth="613.0" styleClass="bodybg" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
      <stylesheets>
         <URL value="@style.css" />
      </stylesheets>
      <Button fx:id="ShowButton" layoutX="225.0" layoutY="188.0" mnemonicParsing="false" onAction="#ShowCatalogue" prefHeight="35.0" prefWidth="150.0" style="-fx-border-color: #c92e9d; -fx-background-color: babypink;" text="Go to Catalogue" textFill="#9a2178" textOverrun="WORD_ELLIPSIS">
         <font>
            <Font name="Arial" size="16.0" />
         </font></Button>
      <Button fx:id="signInButton" layoutX="255.0" layoutY="367.0" mnemonicParsing="false" onAction="#signIn" prefHeight="31.0" prefWidth="90.0" style="-fx-background-color: babypink; -fx-border-color: #c92e9d;" text="Sign In" textFill="#c71692">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <effect>
            <Glow level="0.0" />
         </effect></Button>
      <TextField fx:id="usernameTF" layoutX="198.0" layoutY="295.0" prefHeight="23.0" prefWidth="217.0" style="-fx-background-color: babypink; -fx-border-color: #ef36b8;" />
      <TextField fx:id="passwordTF" layoutX="198.0" layoutY="333.0" prefHeight="25.0" prefWidth="217.0" style="-fx-background-color: babypink; -fx-border-color: #ef36b8;" />
      <Button fx:id="signIpButton" cache="true" layoutX="256.0" layoutY="473.0" mnemonicParsing="false" onAction="#signUp" prefHeight="31.0" prefWidth="90.0" style="-fx-background-color: babypink; -fx-border-color: #c92e9d;" text="Sign Up" textFill="#c52397" />
      <Label layoutX="120.0" layoutY="299.0" text="UserName" textFill="#a83a9f">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </Label>
      <Label layoutX="122.0" layoutY="333.0" prefHeight="25.0" prefWidth="69.0" text="Password" textFill="#b52e9f">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </Label>
      <Label fx:id="instructions" layoutX="170.0" layoutY="443.0" prefHeight="21.0" prefWidth="262.0" text="Don't have an account? Press &quot;Sign Up&quot; " textFill="#c92e9d">
